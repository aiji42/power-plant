{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "cross-env NODE_ENV=production tailwindcss -o ./app/tailwind.css && remix build",
    "postinstall": "remix-esbuild-override",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "dev": "remix build && run-p dev:*",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "deploy": "npm run build && wrangler publish"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.7",
    "@emotion/react": "^11",
    "@emotion/server": "^11",
    "@emotion/styled": "^11",
    "@remix-run/cloudflare": "^1.3.5",
    "@remix-run/cloudflare-workers": "^1.3.5",
    "@remix-run/react": "^1.3.5",
    "@supabase/supabase-js": "^1.33.3",
    "aws4fetch": "^1.0.13",
    "chunk": "^0.0.3",
    "fast-xml-parser": "^4.0.7",
    "framer-motion": "^6",
    "kv-cacheable": "^1.1.1",
    "node-html-parser": "^5.3.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-intersection-observer": "^8.33.1",
    "react-swipeable": "^6.2.1",
    "remix-auth": "^3.2.1",
    "remix-auth-supabase": "^3.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.4.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@remix-run/dev": "^1.3.5",
    "@types/chunk": "^0.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/uuid": "^8.3.4",
    "cross-env": "^7.0.3",
    "miniflare": "^2.4.0",
    "npm-run-all": "^4.1.5",
    "remix-esbuild-override": "^3.0.2",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "main": "build/index.js"
}

{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "dev": "concurrently \"npm run dev:css\" \"remix watch\"",
    "postinstall": "remix setup cloudflare-workers",
    "build:worker": "NODE_ENV=production node worker.build.js",
    "dev:worker": "node worker.build.js",
    "start": "miniflare --build-command \"npm run dev:worker\" --watch",
    "deploy": "npm run build && wrangler publish",
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch"
  },
  "dependencies": {
    "@remix-run/cloudflare-workers": "^1.1.1",
    "@remix-run/react": "^1.1.1",
    "@supabase/supabase-js": "^1.28.6",
    "aws4fetch": "^1.0.13",
    "chunk": "^0.0.3",
    "node-html-parser": "^5.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.2.0",
    "@remix-run/dev": "^1.1.1",
    "@types/chunk": "^0.0.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/uuid": "^8.3.3",
    "concurrently": "^6.5.1",
    "dotenv": "^10.0.0",
    "esbuild": "0.14.6",
    "esbuild-plugin-alias": "^0.2.1",
    "miniflare": "2.0.0-next.3",
    "tailwindcss": "^3.0.7",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "main": "dist/worker.js"
}

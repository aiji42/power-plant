{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "cross-env NODE_ENV=production tailwindcss -o ./app/tailwind.css && remix build",
    "postinstall": "remix-esbuild-override && npm run generate",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "dev": "remix build && run-p dev:*",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "deploy": "npm run deploy:bindee && npm run deploy:edge",
    "deploy:edge": "wrangler publish -c wrangler.edge.toml",
    "deploy:bindee": "wrangler publish -c wrangler.bindee.toml",
    "generate": "PRISMA_CLIENT_ENGINE_TYPE=dataproxy npx prisma generate"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.8.8",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11",
    "@emotion/server": "^11",
    "@emotion/styled": "^11",
    "@prisma/client": "3.14.0",
    "@remix-run/cloudflare": "^1.4.3",
    "@remix-run/cloudflare-workers": "^1.4.3",
    "@remix-run/react": "^1.4.3",
    "@sb-prisma/client": "^2.1.0",
    "@supabase/supabase-js": "^1.35.3",
    "aws4fetch": "^1.0.13",
    "chunk": "^0.0.3",
    "fast-xml-parser": "^4.0.7",
    "framer-motion": "^6.3.3",
    "kv-cacheable": "^1.1.1",
    "node-html-parser": "^5.3.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-intersection-observer": "^9.1.0",
    "react-swipeable": "^7.0.0",
    "remix-auth": "^3.2.1",
    "remix-auth-supabase": "^4.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.9.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@remix-run/dev": "^1.4.3",
    "@types/chunk": "^0.0.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "cross-env": "^7.0.3",
    "esbuild-plugin-alias": "^0.2.1",
    "miniflare": "^2.4.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^3.14.0",
    "remix-esbuild-override": "^3.0.4",
    "remix-service-bindings": "^1.1.1",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4",
    "wrangler": "^2.0.5"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "main": "build/index.js"
}
